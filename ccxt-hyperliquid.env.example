# CCXT Hyperliquid Configuration Example
# Copy these variables to your .env file and replace the placeholder values with your actual credentials

# Exchange identifier - should be 'hyperliquid' for Hyperliquid integration
CCXT_EXCHANGE=hyperliquid

# Your Hyperliquid API key
CCXT_API_KEY=your_hyperliquid_api_key_here

# Your Hyperliquid API secret/private key
CCXT_API_SECRET=your_hyperliquid_private_key_here

# Your Ethereum wallet address associated with Hyperliquid
CCXT_WALLET_ADDRESS=your_ethereum_wallet_address

# Set to 'true' for testnet, 'false' for mainnet
CCXT_TESTNET=true

# Optional: Additional parameters that might be required for specific Hyperliquid operations
# CCXT_ADDITIONAL_PARAM1=value1
# CCXT_ADDITIONAL_PARAM2=value2
